<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/LoginController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>Controller.LogoutController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/LogoutController</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PopulateTable</servlet-name>
        <servlet-class>Topic.PopulateTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PopulateTable</servlet-name>
        <url-pattern>/PopulateTable</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AddAsset</servlet-name>
        <servlet-class>Topic.AddAsset</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAsset</servlet-name>
        <url-pattern>/AddAsset</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FilterTable</servlet-name>
        <servlet-class>Topic.FilterTable</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FilterTable</servlet-name>
        <url-pattern>/FilterTable</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>Update</servlet-name>
        <servlet-class>Topic.Update</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Update</servlet-name>
        <url-pattern>/Update</url-pattern>
    </servlet-mapping>


</web-app>
